{
  "name": "livedex-tracker",
  "xo": {
    "envs": [
      "node",
      "browser"
    ],
    "space": true,
    "plugins": [
      "immutable"
    ],
    "extends": "xo-react",
    "rules": {
      "react/jsx-space-before-closing": [
        2,
        "always"
      ],
      "import/no-extraneous-dependencies": 0,
      "no-prototype-builtins": 0
    }
  },
  "version": "0.0.0",
  "description": "A living pokedex tracker that supports building it in any gen",
  "main": "",
  "scripts": {
    "start": "node dev-server.js",
    "test": "xo && ava",
    "dev-build": "webpack -d --config webpack.config.dev",
    "build": "webpack -p --config webpack.config.production",
    "get-info": "node ./tools/get-pokemon-info.js",
    "make-atlas": "babel-node ./tools/make-atlas.js"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Dan E",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.25.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "gm": "^1.23.0",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "node-sass": "^3.10.1",
    "postcss-loader": "^1.0.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "superagent": "^3.3.1",
    "superagent-charset": "^1.1.1",
    "webpack": "^2.1.0-beta",
    "webpack-dev-server": "^2.1.0-beta",
    "xo": "^0.17.0"
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "eslint-plugin-react": "^6.7.1",
    "immutability-helper": "^2.0.0",
    "immutable": "^3.8.1",
    "immutable-js": "^0.3.1-6",
    "immutable-props": "^1.1.0",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-hot-loader": "^3.0.0-beta.5",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.11",
    "xo": "^0.17.1"
  }
}
